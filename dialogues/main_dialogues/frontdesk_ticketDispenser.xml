<DialogueTree xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/xen-42/outer-wilds-new-horizons/main/NewHorizons/Schemas/dialogue_schema.xsd">
	<NameField>SIGN</NameField>

	<!-- CONDITION/SHIPLOG
		- CALLISTHESIS_CONDITION_TICKET_01;						Set in START_DEFAULT, Used in START_TICKET_01
		- CALLISTHESIS_CONDITION_TICKET_02;						Set in START_TICKET_01, Used in START_TICKET_02
		- CALLISTHESIS_CONDITION_TICKET_03;						Set in START_TICKET_02, Used in START_TICKET_03
		- CALLISTHESIS_CONDITION_TICKET_04;						Set in START_TICKET_03, Used in START_TICKET_04
		- CALLISTHESIS_CONDITION_TICKET_05;						Set in START_TICKET_04, Used in START_TICKET_05
		- CALLISTHESIS_CONDITION_TICKET_ALL;					Set in START_TICKET_05

		- CALLISTHESIS_CONDITION_FRONTDESK_LUNCHBREAK;			Set in END_DEPLETED_TICKETS_2
	-->

	<!-- ENTRY NODES-->
	<DialogueNode>
		<Name>START_DEFAULT</Name>
		<EntryCondition>DEFAULT</EntryCondition>
		<Dialogue>
			<Page>Currently serving ticket #F102</Page>
		</Dialogue>

		<DialogueOptionsList>
			<DialogueOption>
				<Text>*Grab a ticket*</Text>
				<ConditionToSet>CALLISTHESIS_CONDITION_TICKET_01</ConditionToSet>
				<DialogueTarget>TICKET_GRAB_NONE</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>

	<DialogueNode>
		<Name>START_TICKET_01</Name>
