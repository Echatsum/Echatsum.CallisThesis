		</Dialogue>

		<DialogueOptionsList>
			<DialogueOption>
				<Text>*Check my ticket*</Text>
				<DialogueTarget>TICKET_CHECK_01</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>*Grab a ticket*</Text>
				<ConditionToSet>CALLISTHESIS_CONDITION_TICKET_02</ConditionToSet>
				<ConditionToCancel>CALLISTHESIS_CONDITION_TICKET_01</ConditionToCancel>
				<DialogueTarget>TICKET_GRAB_01</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>

