			<DialogueOption>
				<Text>*Check my ticket*</Text>
				<DialogueTarget>TICKET_CHECK_05</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>*GRAB ALL THE TICKETS*</Text>
				<ConditionToSet>CALLISTHESIS_CONDITION_TICKET_ALL</ConditionToSet> <!-- Probably useless now -->
				<ConditionToCancel>CALLISTHESIS_CONDITION_TICKET_05</ConditionToCancel>
				<DialogueTarget>TICKET_GRAB_ALL</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>


	<!-- TICKET_GRAB/TICKET_CHECK NODES-->

	<!-- TICKET_01: K594, 2h12min-->
	<DialogueNode>
		<Name>TICKET_GRAB_NONE</Name>
		<Dialogue>
			<Page>Dispensed ticket #K594. Estimated waiting time 2h12min.</Page>
		</Dialogue>
		<DialogueTarget>EXIT</DialogueTarget>
	</DialogueNode>
	<DialogueNode>
		<Name>TICKET_CHECK_01</Name>
		<Dialogue>
			<Page>Your ticket is #K594. Estimated waiting time 2h12min.</Page>
		</Dialogue>
		<DialogueTarget>EXIT</DialogueTarget>
	</DialogueNode>
	
