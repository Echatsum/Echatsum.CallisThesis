	<!-- TICKET_ALL: A103, 1min-->
	<DialogueNode>
		<Name>TICKET_GRAB_ALL</Name>
		<Dialogue>
			<Page>*Among the tickets, you find ticket #A103*</Page>
		</Dialogue>
		<DialogueTarget>END_DEPLETED_TICKETS_1</DialogueTarget>
	</DialogueNode>



	<!-- EXIT NODES-->
	<DialogueNode>
		<Name>EXIT</Name>
		<Dialogue>
			<Page>We thank you for your patience.</Page>
		</Dialogue>
	</DialogueNode>
	
	<DialogueNode>
		<Name>END_DEPLETED_TICKETS_1</Name>
		<Dialogue>
			<Page>Currently serving ticket #A103</Page>
			<Page>Warning! This unit has ran out of paper and cannot dispense more tickets. Please notify the nearest employee for a paper refill.</Page>
		</Dialogue>
		
		<DialogueOptionsList>
			<DialogueOption>
				<Text>*Check my ticket*</Text>
				<DialogueTarget>END_DEPLETED_TICKETS_2</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>

	<DialogueNode>
		<Name>END_DEPLETED_TICKETS_2</Name>
		<Dialogue>
			<Page>Your ticket is #A103. You can proceed to the front desk.</Page>
		</Dialogue>
		<DialogueOptionsList>
			<DialogueOption>
				<Text>Finally.</Text>
				<ConditionToSet>CALLISTHESIS_CONDITION_FRONTDESK_LUNCHBREAK</ConditionToSet>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>

</DialogueTree>