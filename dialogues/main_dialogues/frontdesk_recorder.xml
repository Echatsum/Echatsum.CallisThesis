		</Dialogue>
		<DialogueOptionsList>
			<DialogueOption>
				<Text>*1-1-1-1*</Text>
				<DialogueTarget>SEARCH_AUTHOR_PASSWORD_FAIL</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>*1-2-3-4*</Text>
				<DialogueTarget>SEARCH_AUTHOR_PASSWORD_FAIL</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>*P-A-S-S-W-O-R-D-1*</Text>
				<DialogueTarget>SEARCH_AUTHOR_PASSWORD_FAIL</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>*slam head on keyboard*</Text>
				<DialogueTarget>SEARCH_AUTHOR_PASSWORD_FAIL</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<RequiredLogCondition>CALLISTHESIS_SHIPLOG_PASSWORD_CLUES</RequiredLogCondition>
				<RequiredLogCondition>CALLISTHESIS_SHIPLOG_ARCHIVE_SIGNAL</RequiredLogCondition>
				<RequiredLogCondition>CALLISTHESIS_SHIPLOG_PASSWORD_PART_3_USELESS_2</RequiredLogCondition>
				<Text>*1-0-3-1-7-1-1-5-6-2-6*</Text>
				<DialogueTarget>SEARCH_AUTHOR_PASSWORD_FAIL</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<RequiredLogCondition>CALLISTHESIS_SHIPLOG_PASSWORD_CLUES</RequiredLogCondition>
				<RequiredLogCondition>CALLISTHESIS_SHIPLOG_ARCHIVE_SIGNAL</RequiredLogCondition>
				<RequiredLogCondition>CALLISTHESIS_SHIPLOG_PASSWORD_PART_3_USELESS_4</RequiredLogCondition>
				<Text>*1-0-3-1-7-1-1-5-6-2-2*</Text>
				<DialogueTarget>SEARCH_AUTHOR_PASSWORD_FAIL</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<RequiredLogCondition>CALLISTHESIS_SHIPLOG_PASSWORD_CLUES</RequiredLogCondition>
				<RequiredLogCondition>CALLISTHESIS_SHIPLOG_ARCHIVE_SIGNAL</RequiredLogCondition>
				<RequiredLogCondition>CALLISTHESIS_SHIPLOG_PASSWORD_PART_3_USELESS_1</RequiredLogCondition>
				<Text>*1-0-3-1-7-1-1-5-6-2-4*</Text>
				<DialogueTarget>SEARCH_AUTHOR_PASSWORD_FAIL</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<RequiredLogCondition>CALLISTHESIS_SHIPLOG_PASSWORD_CLUES</RequiredLogCondition>
				<RequiredLogCondition>CALLISTHESIS_SHIPLOG_ARCHIVE_SIGNAL</RequiredLogCondition>
				<RequiredLogCondition>CALLISTHESIS_SHIPLOG_PASSWORD_PART_3_USELESS_5</RequiredLogCondition>
				<Text>*1-0-3-1-7-1-1-5-6-2-5*</Text>
				<DialogueTarget>SEARCH_AUTHOR_PASSWORD_FAIL</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<RequiredLogCondition>CALLISTHESIS_SHIPLOG_PASSWORD_CLUES</RequiredLogCondition>
				<RequiredLogCondition>CALLISTHESIS_SHIPLOG_ARCHIVE_SIGNAL</RequiredLogCondition>
				<RequiredLogCondition>CALLISTHESIS_SHIPLOG_PASSWORD_PART_3_ANSWER</RequiredLogCondition>
				<Text>*1-0-3-1-7-1-1-5-6-2-0*</Text>
				<DialogueTarget>SEARCH_AUTHOR_PASSWORD_SUCCESS</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>

	<DialogueNode>
		<Name>SEARCH_AUTHOR_PASSWORD_SUCCESS</Name>
		<Dialogue>
			<Page>Password valid. Welcome user [Callis]</Page>
			<Page>Would you like to download a copy of your scroll?</Page>
		</Dialogue>
		<DialogueOptionsList>
			<DialogueOption>
				<Text>*Yes*</Text>
				<DialogueTarget>SEARCH_DOWNLOAD_THESIS_1</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>*No*</Text>
				<DialogueTarget>SEARCH_END</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>
	
	<DialogueNode>
		<Name>SEARCH_AUTHOR_PASSWORD_FAIL</Name>
		<Dialogue>
			<Page>Invalid password entered. Entries cannot be accessed, we apologize for the inconvenience.</Page>
			<Page>Returning to this unit main menu.</Page>
		</Dialogue>
		<RevealFacts>
			<FactID>CALLISTHESIS_SHIPLOG_PASSWORD_RUMOR_ARCHIVE_LINK</FactID>
			<FactID>CALLISTHESIS_SHIPLOG_PASSWORD_FACT</FactID>
		</RevealFacts>
		<DialogueTarget>START_DEFAULT</DialogueTarget>
	</DialogueNode>
	
	<DialogueNode>
		<Name>SEARCH_AUTHOR_2_2</Name>
		<Dialogue>
			<Page>Thank you. Searching database for entries..</Page>
			<Page>..</Page>
			<Page>..</Page>
			<Page>No scroll found under this author name.</Page>
			<Page>Returning to this unit main menu.</Page>
		</Dialogue>
		<DialogueTarget>START_DEFAULT</DialogueTarget>
	</DialogueNode>
	
	<DialogueNode>
		<Name>SEARCH_AUTHOR_2_3</Name>
