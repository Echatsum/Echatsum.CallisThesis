		<Name>SEARCH_AUTHOR_2_4</Name>
		<Dialogue>
			<Page>Thank you. Searching database for entries..</Page>
			<Page>..</Page>
			<Page>..</Page>
			<Page>[68 entries] found in the database.</Page>
			<Page>This includes [1 entry] present in the rotating selection for public reading [4F5]</Page>
			<Page>Would you like to download one of the entries onto a new scroll?</Page>
		</Dialogue>
		<DialogueOptionsList>
			<DialogueOption>
				<Text>*Yes*</Text>
				<DialogueTarget>SEARCH_DOWNLOAD</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>*No*</Text>
				<DialogueTarget>SEARCH_END</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>

	
	<DialogueNode>
		<Name>SEARCH_KEYWORD_1</Name>
		<Dialogue>
			<Page>Please type a keyword on the keypad.</Page>
		</Dialogue>
		<DialogueOptionsList>
			<DialogueOption>
				<RequiredLogCondition>CALLISTHESIS_SHIPLOG_PASSWORD_PART_3_CLUE</RequiredLogCondition>
