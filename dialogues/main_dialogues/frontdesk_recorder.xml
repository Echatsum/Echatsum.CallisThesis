			</DialogueOption>
			<DialogueOption>
				<Text>*S-i-g-n-a-l-s*</Text>
				<DialogueTarget>SEARCH_KEYWORD_2_2</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>

	<DialogueNode>
		<Name>SEARCH_KEYWORD_2_1</Name>
