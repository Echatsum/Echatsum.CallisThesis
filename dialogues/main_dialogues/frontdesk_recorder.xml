<DialogueTree xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/xen-42/outer-wilds-new-horizons/main/NewHorizons/Schemas/dialogue_schema.xsd">
	<NameField>SIGN</NameField>

	<!-- CONDITION/SHIPLOG
		- CALLISTHESIS_CONDITION_GUARD_REFUSAL;					Set in START_DEFAULT, Used in START_A

		- CALLISTHESIS_SHIPLOG_PASSWORD_FACT;					Used in SEARCH_AUTHOR_PASSWORD_FAIL
		- CALLISTHESIS_SHIPLOG_PASSWORD_RUMOR_ARCHIVE_LINK;		Used in SEARCH_AUTHOR_PASSWORD_FAIL

		- CALLISTHESIS_PASSWORD_PART_2_ANSWER;					Used in SEARCH_AUTHOR_2_1	
		- CALLISTHESIS_PASSWORD_PART_3_ANSWER;					Used in SEARCH_AUTHOR_2_1	
	-->

	<!-- ENTRY NODES-->
	<DialogueNode>
		<Name>START_DEFAULT</Name>
		<EntryCondition>DEFAULT</EntryCondition>
		<Dialogue>
			<Page>This unit is an automated helper for the Nomai Archives.</Page>
			<Page>To add a scroll or tablet to our expanding archive, press 1.</Page>
			<Page>To consult an existing scroll or tablet, press 2.</Page>
			<Page>To conclude the interaction with this unit, press 3.</Page>
			<Page>To list your options once more, press 4.</Page>
		</Dialogue>
		<DialogueOptionsList>
			<DialogueOption>
				<Text>*Press 1*</Text>
				<DialogueTarget>ADD_EDIT_1</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>*Press 2*</Text>
				<DialogueTarget>SEARCH_1</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>*Press 3*</Text>
			</DialogueOption>
			<DialogueOption>
				<Text>*Press 4*</Text>
				<DialogueTarget>START_DEFAULT</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>	

	<!-- ADD_EDIT NODES-->
	<DialogueNode>
		<Name>ADD_EDIT_1</Name>
		<Dialogue>
			<Page>We are currently experiencing severe data loss on upload.</Page>
			<Page>As such, all addition or edit to the archives must be supervised by a handler.</Page>
			<Page>We apologize for the inconvenience and assure you this measure is to protect your legacies stored here.</Page>
			<Page>We advise that you wait for a handler to assist you.</Page>
			<Page>Returning to this unit main menu.</Page>
		</Dialogue>
		<DialogueTarget>START_DEFAULT</DialogueTarget>
	</DialogueNode>
	

	<!-- SEARCH NODES-->
	<DialogueNode>
		<Name>SEARCH_1</Name>
		<Dialogue>
			<Page>You are looking for an existing scroll or tablet.</Page>
			<Page>If you are looking for a specific author, press 1.</Page>
			<Page>If you want to search using a specific keyword, press 2.</Page>
			<Page>To return to this unit's main menu, press 3.</Page>
			<Page>To list your options once more, press 4.</Page>
		</Dialogue>
		<DialogueOptionsList>
			<DialogueOption>
				<Text>*Press 1*</Text>
				<DialogueTarget>SEARCH_AUTHOR_1</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>*Press 2*</Text>
				<DialogueTarget>SEARCH_KEYWORD_1</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>*Press 3*</Text>
				<DialogueTarget>START_DEFAULT</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>*Press 4*</Text>
				<DialogueTarget>SEARCH_1</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>
	
	<DialogueNode>
		<Name>SEARCH_AUTHOR_1</Name>
		<Dialogue>
			<Page>Please enter the author name on the keypad.</Page>
		</Dialogue>
		<DialogueOptionsList>
			<DialogueOption>
				<RequiredLogCondition>CALLISTHESIS_SHIPLOG_FESTIVAL_EXPLORE_CALLIS</RequiredLogCondition>
				<Text>*C-a-l-l-i-s*</Text>
				<DialogueTarget>SEARCH_AUTHOR_2_1</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>*A-s-t-i-l-b-e*</Text>
				<DialogueTarget>SEARCH_AUTHOR_2_2</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>*A-l-l-i-u-m*</Text>
				<DialogueTarget>SEARCH_AUTHOR_2_3</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>*A-s-t-e-r*</Text>
				<DialogueTarget>SEARCH_AUTHOR_2_4</DialogueTarget>
			</DialogueOption>
			<DialogueOption>
				<Text>*S-o-l-a-n-u-m*</Text>
				<DialogueTarget>SEARCH_AUTHOR_2_2</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>

	<DialogueNode>
		<Name>SEARCH_AUTHOR_2_1</Name>
